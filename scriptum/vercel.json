{
    "version": 2,
    "builds": [{
        "src": "scriptum/wsgi.py",
        "use": "@vercel/python"
    }],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "scriptum/wsgi.py"
        }
    ],
    "env": {
        "PYTHON_VERSION": "3.9"
    }
}